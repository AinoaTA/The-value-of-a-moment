      en: 
  - id: IRopaSucia
    lines:
    - ID: IRopaSucia_E1
      dialogue: IRopaSucia
      voice_es: {fileID: 0}
      voice_en: {fileID: 0}
      es: "Dentro de poco no me quedar\xE1 ropa limpia."
      en: 
